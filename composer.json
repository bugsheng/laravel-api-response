{
    "name": "bugsheng/laravel-api-response",
	"description": "Laravel Lumen response api",
    "license": "MIT",
    "authors": [
        {
            "name": "BugSheng",
            "email": "bug_sheng@126.com"
        }
    ],
    "require": {
      "php": "^7.0|^8.0",
      "illuminate/http": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0",
      "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
	"autoload": {
		"psr-4": {
            "BugSheng\\Laravel\\ApiResponse\\": "src/"
        }
	},
	"minimum-stability": "dev",
	"extra": {
		"laravel": {
            "providers": [
              "BugSheng\\Laravel\\ApiResponse\\ServiceProvider"
            ],
            "aliases": {
              "ApiRes": "BugSheng\\Laravel\\ApiResponse\\Facade"
            }
        }
	}
}
